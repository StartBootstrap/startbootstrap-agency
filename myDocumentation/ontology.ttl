@prefix : <http://www.semanticweb.org/lisa/ontologies/2019/7/rap#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/lisa/ontologies/2019/7/rap> .

<http://www.semanticweb.org/lisa/ontologies/2019/7/rap> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty ;
                                        rdfs:subPropertyOf rdfs:comment ;
                                        rdfs:range xsd:string ;
                                        rdfs:domain :Musical_Work .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#definition
:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#collaborated_on_song
:collaborated_on_song rdf:type owl:ObjectProperty ;
                      owl:inverseOf :features ;
                      rdf:type owl:InverseFunctionalProperty ;
                      rdfs:domain :Person ;
                      rdfs:range :Musical_Work ;
                      rdfs:comment "connects class Person to class Musical work." ;
                      rdfs:label "collaborated_on_song" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#features
:features rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Musical_Work ;
          rdfs:range :Person ;
          rdfs:comment "featuring the collaborated artist while making the musical work." ;
          rdfs:label "features" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_album
:has_album rdf:type owl:ObjectProperty ;
           owl:inverseOf :was_issued_by ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Person ;
           rdfs:range :Album ;
           rdfs:comment "property that connects classes of the ontology to the class Album." ;
           rdfs:label "has_album" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_compound_sentiment_score
:has_compound_sentiment_score rdf:type owl:ObjectProperty ;
                              owl:inverseOf :is_compound_sentiment_score_of ;
                              rdf:type owl:InverseFunctionalProperty ;
                              rdfs:domain :Musical_Work ;
                              rdfs:range :Compound ;
                              rdfs:comment "compound sentiment score obtained by vader." ;
                              rdfs:label "has_compound_sentiment_score" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_genre
:has_genre rdf:type owl:ObjectProperty ;
           owl:inverseOf :is_genre_of ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Album
                                       :Musical_Work
                                       :Person
                                     )
                       ] ;
           rdfs:range <http://purl.org/ontology/mo/Genre> ;
           rdfs:comment "property that connects class Genre to the classes Album or MusicalWork or Person." ;
           rdfs:label "has_genre" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_literary_device
:has_literary_device rdf:type owl:ObjectProperty ;
                     owl:inverseOf :technique_used_in ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain :Musical_Work ;
                     rdfs:range :Literary_device ;
                     rdfs:comment "property that connects class Musical_Work to the class Literary_Device" ;
                     rdfs:label "has_literary_device" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_lyrics
:has_lyrics rdf:type owl:ObjectProperty ;
            owl:inverseOf :is_lyric_of ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Musical_Work ;
            rdfs:range :Lyrics ;
            rdfs:comment "property that connects classes of the ontology to the Lyrics." ;
            rdfs:label "has_lyrics" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_negative_sentiment
:has_negative_sentiment rdf:type owl:ObjectProperty ;
                        owl:inverseOf :is_neg_sentiment_score_of ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain :Musical_Work ;
                        rdfs:range :Negative ;
                        rdfs:comment "negative sentiment score" ;
                        rdfs:label "has_negative_sentiment" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_neutral_sentiment_score
:has_neutral_sentiment_score rdf:type owl:ObjectProperty ;
                             owl:inverseOf :is_neutral_sentiment_score_of ;
                             rdf:type owl:FunctionalProperty ;
                             rdfs:domain :Neutral ;
                             rdfs:range :Musical_Work ;
                             rdfs:comment "neutral sentiment score obtained by VADER." ;
                             rdfs:label "has_neutral_sentiment_score" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_place
:has_place rdf:type owl:ObjectProperty ;
           owl:inverseOf :is_place_of ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Person ;
           rdfs:range :Place ;
           rdfs:comment "property that connects classes of the ontology to the class Place" ;
           rdfs:label "has_place" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_positive_sentiment
:has_positive_sentiment rdf:type owl:ObjectProperty ;
                        owl:inverseOf :is_pos_sentiment_score_of ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain :Musical_Work ;
                        rdfs:range :Sentiment ;
                        rdfs:comment "property that connects classes of the ontology to the class Sentiment." ;
                        rdfs:label "has_positive_sentiment" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_reference_of
:has_reference_of rdf:type owl:ObjectProperty ;
                  owl:inverseOf :is_referenced_in_song ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Musical_Work ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( :Car
                                             :Person
                                             :Place
                                             :Topic
                                           )
                             ] ;
                  rdfs:comment "property indicatint a musical work that makes has a reference to a topic." ;
                  rdfs:label "has_reference_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_release_date
:has_release_date rdf:type owl:ObjectProperty ;
                  owl:inverseOf :release_date_of ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Album
                                              :Car
                                              :Musical_Work
                                            )
                              ] ;
                  rdfs:range :Release_date ;
                  rdfs:comment "property that connects classes of the ontology to the class Time Span." ;
                  rdfs:label "has_release_date" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_song
:has_song rdf:type owl:ObjectProperty ;
          owl:inverseOf :is_part_of ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Album ;
          rdfs:range :Musical_Work ;
          rdfs:comment "property that connects classes of the ontology to the class MusicalWork." ;
          rdfs:label "has_song" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#has_topic
:has_topic rdf:type owl:ObjectProperty ;
           owl:inverseOf :is_topic_of ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Musical_Work ;
           rdfs:range :Topic ;
           rdfs:comment "property that connects classes of the ontology to the class Topic." ;
           rdfs:label "has_topic" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_compound_sentiment_score_of
:is_compound_sentiment_score_of rdf:type owl:ObjectProperty ,
                                         owl:FunctionalProperty ;
                                rdfs:domain :Compound ;
                                rdfs:range :Musical_Work ;
                                rdfs:comment "compound sentiment score obtained by VADER that belogs to a musical work." ;
                                rdfs:label "is_compound_sentiment_score_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_genre_of
:is_genre_of rdf:type owl:ObjectProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain <http://purl.org/ontology/mo/Genre> ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Album
                                        :Musical_Work
                                        :Person
                                      )
                        ] ;
             rdfs:comment "property that connects class genre to the classes Album or MusicalWork or Person" ;
             rdfs:label "is_genre_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_lyric_of
:is_lyric_of rdf:type owl:ObjectProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain :Lyrics ;
             rdfs:range :Musical_Work ;
             rdfs:comment "property that connects class Lyrics to the class Song." ;
             rdfs:label "is_lyric_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_neg_sentiment_score_of
:is_neg_sentiment_score_of rdf:type owl:ObjectProperty ,
                                    owl:InverseFunctionalProperty ;
                           rdfs:domain :Negative ;
                           rdfs:range :Musical_Work ;
                           rdfs:comment "negative sentiment of a song" ;
                           rdfs:label "is_neg_sentiment_score_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_neutral_sentiment_score_of
:is_neutral_sentiment_score_of rdf:type owl:ObjectProperty ,
                                        owl:InverseFunctionalProperty ;
                               rdfs:domain :Musical_Work ;
                               rdfs:range :Neutral ;
                               rdfs:comment "neutral sentiment score obtained by VADER." ;
                               rdfs:label "is_neutral_sentiment_score_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_part_of
:is_part_of rdf:type owl:ObjectProperty ,
                     owl:InverseFunctionalProperty ;
            rdfs:domain :Musical_Work ;
            rdfs:range :Album ;
            rdfs:comment "property that connects class MusicWork to the class Album." ;
            rdfs:label "is_part_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_place_of
:is_place_of rdf:type owl:ObjectProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain :Place ;
             rdfs:range :Person ;
             rdfs:comment "the property that show a relation between class PLace and class Person." ;
             rdfs:label "is_place_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_pos_sentiment_score_of
:is_pos_sentiment_score_of rdf:type owl:ObjectProperty ,
                                    owl:InverseFunctionalProperty ;
                           rdfs:domain :Sentiment ;
                           rdfs:range :Musical_Work ;
                           rdfs:comment "the property that relates class Sentiment to the class MusicalWork" ;
                           rdfs:label "is_pos_sentiment_score_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_referenced_by
:is_referenced_by rdf:type owl:ObjectProperty ;
                  owl:inverseOf :references ;
                  rdf:type owl:InverseFunctionalProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Car
                                              :Person
                                              :Place
                                              :Topic
                                            )
                              ] ;
                  rdfs:range :Person ;
                  rdfs:comment "property that connects the classes Topic or Place or Car or Person to the class Person." ;
                  rdfs:label "is_referenced_by" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_referenced_in_song
:is_referenced_in_song rdf:type owl:ObjectProperty ,
                                owl:InverseFunctionalProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :Car
                                                   :Person
                                                   :Place
                                                   :Topic
                                                 )
                                   ] ;
                       rdfs:range :Musical_Work ;
                       rdfs:comment "A topic that was referenced in a musical work." ;
                       rdfs:label "is_referenced_in_song" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#is_topic_of
:is_topic_of rdf:type owl:ObjectProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain :Topic ;
             rdfs:range :Musical_Work ;
             rdfs:comment "property that connects class Topic to the class Musical_Work" ;
             rdfs:label "is_topic_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#made_song
:made_song rdf:type owl:ObjectProperty ;
           owl:inverseOf :song_made_by ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Person ;
           rdfs:range :Musical_Work ;
           rdfs:comment "property that connects class Person to class Musical_Work" ;
           rdfs:label "made_song" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#references
:references rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Person ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Car
                                       :Person
                                       :Place
                                       :Topic
                                     )
                       ] ;
            rdfs:comment "property that connects classes Topic or Place or Car or Person of the ontology to the class Topic." ;
            rdfs:label "references" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#release_date_of
:release_date_of rdf:type owl:ObjectProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain :Release_date ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( :Album
                                            :Car
                                            :Musical_Work
                                          )
                            ] ;
                 rdfs:comment "property that connects class Time_Span to the class Album or Car or Musical_Work or Person." ;
                 rdfs:label "release_date_of" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#song_made_by
:song_made_by rdf:type owl:ObjectProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain :Musical_Work ;
              rdfs:range :Person ;
              rdfs:comment "the relation of class Musical_Work to class Person." ;
              rdfs:label "song_made_by" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#technique_used_by
:technique_used_by rdf:type owl:ObjectProperty ;
                   owl:inverseOf :uses_literary_device ;
                   rdf:type owl:InverseFunctionalProperty ;
                   rdfs:domain :Literary_device ;
                   rdfs:range :Person ;
                   rdfs:comment "property that connect class Literary_Device to the class Person" ;
                   rdfs:label "technique_used_by" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#technique_used_in
:technique_used_in rdf:type owl:ObjectProperty ,
                            owl:InverseFunctionalProperty ;
                   rdfs:domain :Literary_device ;
                   rdfs:range :Musical_Work ;
                   rdfs:comment "property that connect class Literary_Device to the class Musical_Work" ;
                   rdfs:label "technique_used_in" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#uses_literary_device
:uses_literary_device rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain :Person ;
                      rdfs:range :Literary_device ;
                      rdfs:comment "property that connects the class Person to the class Literary Device" ;
                      rdfs:label "uses_literary_device" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#was_issued_by
:was_issued_by rdf:type owl:ObjectProperty ,
                        owl:InverseFunctionalProperty ;
               rdfs:domain :Album ;
               rdfs:range :Person ;
               rdfs:comment "the property that connects class Album to class Person." ;
               rdfs:label "was_issued_by" .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdf:type owl:FunctionalProperty .


#################################################################
#    Data properties
#################################################################

###  http://dbpedia.org/ontology/City
<http://dbpedia.org/ontology/City> rdf:type owl:DatatypeProperty ;
                                   rdfs:domain :City ;
                                   rdfs:range xsd:string ;
                                   rdfs:comment "is a large human settlement." ;
                                   rdfs:label "dbo:City" .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#sentiment_score
:sentiment_score rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Musical_Work ;
                 rdfs:range xsd:int ;
                 rdfs:comment "the sentiment score of a musical work (positive, negative, compound)." ;
                 rdfs:label "sentiment_score" .


###  http://xmlns.com/foaf/0.1/maker
<http://xmlns.com/foaf/0.1/maker> rdf:type owl:DatatypeProperty ;
                                  rdfs:domain [ rdf:type owl:Class ;
                                                owl:unionOf ( :Album
                                                              :Musical_Work
                                                            )
                                              ] ;
                                  rdfs:range xsd:string ;
                                  rdfs:comment "the creator of a musical work." ;
                                  rdfs:label "foaf:maker" .


###  http://xmlns.com/foaf/0.1/member
<http://xmlns.com/foaf/0.1/member> rdf:type owl:DatatypeProperty ;
                                   rdfs:domain :Music_Group ;
                                   rdfs:range xsd:string ;
                                   rdfs:comment "a member of a music band." ;
                                   rdfs:label "foaf:member" .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdf:type owl:DatatypeProperty ;
                                 rdfs:domain :Person ;
                                 rdfs:range xsd:string ;
                                 rdfs:comment "a person's name (person could be  real or fictional)." ;
                                 rdfs:label "name" .


#################################################################
#    Classes
#################################################################

###  http://purl.org/ontology/mo/Genre
<http://purl.org/ontology/mo/Genre> rdf:type owl:Class ;
                                    rdfs:label "Genre" ;
                                    <http://www.w3.org/2004/02/skos/core#definition> "An expressive style of music." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Album
:Album rdf:type owl:Class ;
       rdfs:label "Album" ;
       <http://www.w3.org/2004/02/skos/core#definition> "An album is a collection of audio recordings issued as a collection on compact disc (CD), vinyl, audio tape, or another medium." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#BMW
:BMW rdf:type owl:Class ;
     rdfs:subClassOf :Car .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Car
:Car rdf:type owl:Class ;
     rdfs:label "Car" ;
     <http://www.w3.org/2004/02/skos/core#definition> "a road vehicle." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Place ;
      owl:disjointWith :Country ;
      rdfs:label "City" ;
      <http://www.w3.org/2004/02/skos/core#definition> "A relatively large and permanent settlement, particularly a large urban settlement" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Compound
:Compound rdf:type owl:Class ;
          rdfs:subClassOf :Sentiment ;
          rdfs:comment "compound according to VADER Sentiment analysis." ;
          rdfs:label "Compound" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf :Place ;
         rdfs:comment "The country where the thing is located." ;
         rdfs:label "Country" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Imagery
:Imagery rdf:type owl:Class ;
         rdfs:subClassOf :Literary_device ;
         rdfs:label "Imagery" ;
         <http://www.w3.org/2004/02/skos/core#definition> "Imagery is the literary term used for language and description that appeals to our five senses. When a music artist attempts to describe something so that it appeals to our sense of smell, sight, taste, touch, or hearing; he/she has used imagery." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Irony
:Irony rdf:type owl:Class ;
       rdfs:subClassOf :Literary_device ;
       rdfs:label "Irony" ;
       <http://www.w3.org/2004/02/skos/core#definition> "The use of irony in literature refers to playing around with words such that the meaning implied by a sentence or word is actually different from the literal meaning. Often irony is used to suggest the stark contrast of the literal meaning being put forth. The deeper, real layer of significance is revealed not by the words themselves but the situation and the context in which they are placed." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Literary_device
:Literary_device rdf:type owl:Class ;
                 rdfs:label "Literary_Device" ;
                 <http://www.w3.org/2004/02/skos/core#definition> "literary device is a technique a writer uses to produce a special effect in their writing." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Litote
:Litote rdf:type owl:Class ;
        rdfs:subClassOf :Literary_device ;
        rdfs:label "Litote" ;
        <http://www.w3.org/2004/02/skos/core#definition> "Litotes are a figure of speech consisting of an understatement in which an affirmative is expressed via the negative of its contrary." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Lyrics
:Lyrics rdf:type owl:Class ;
        rdfs:label "Lyrics" ;
        <http://www.w3.org/2004/02/skos/core#definition> "Associates lyrics with a musical work" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Mercedes_Benz
:Mercedes_Benz rdf:type owl:Class ;
               rdfs:subClassOf :Car ;
               rdfs:comment "Bayerische Motoren Werke AG, commonly referred to as BMW is a German multinational company which produces automobiles and motorcycles." ;
               rdfs:label "Mercedes_Benz" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Metonymy
:Metonymy rdf:type owl:Class ;
          rdfs:subClassOf :Literary_device ;
          rdfs:label "Metonomy" ;
          <http://www.w3.org/2004/02/skos/core#definition> "A metonymy is a word or phrase that is substituted for a more formal word or phrase to which it is related." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Music_Artist
:Music_Artist rdf:type owl:Class ;
              rdfs:subClassOf :Person ;
              owl:disjointWith :Music_Group ;
              rdfs:label "Music_Artist" ;
              <http://www.w3.org/2004/02/skos/core#definition> "A person or a group of people (or a computer :-) ), whose musical creative work shows sensitivity and imagination." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Music_Group
:Music_Group rdf:type owl:Class ;
             rdfs:subClassOf :Person ;
             rdfs:label "Music_Group" ;
             <http://www.w3.org/2004/02/skos/core#definition> "Group of musicians, or musical ensemble, usually popular or folk, playing parts of or improvising off of a musical arrangement." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Musical_Work
:Musical_Work rdf:type owl:Class ;
              rdfs:label "Musical_Work" ;
              <http://www.w3.org/2004/02/skos/core#definition> "Distinct intellectual or artistic musical creation." .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Negative
:Negative rdf:type owl:Class ;
          rdfs:subClassOf :Sentiment ;
          rdfs:comment "negative according to VADER Sentiment analysis." ;
          rdfs:label "Negative" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Neutral
:Neutral rdf:type owl:Class ;
         rdfs:subClassOf :Sentiment ;
         rdfs:comment "neutral according to VADER Sentiment analysis." ;
         rdfs:label "Neutral" .


###  http://www.semanticweb.org/lisa/ontologies/2019/7/rap#Palindrom
:Palindrom rdf:type owl:Class ;
           rdfs:subClassOf :Literary_device ;
